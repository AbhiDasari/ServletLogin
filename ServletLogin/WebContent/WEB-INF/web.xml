<?xml version="1.0" encoding="UTF-8"?>
<web-app  xmlns:web=" http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" >
  <servlet>
    <servlet-name>OnServletLogin</servlet-name>
    <servlet-class>com.servletlogin.OnServletLogin
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OnServletLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.servletlogin.home
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>